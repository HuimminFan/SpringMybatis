<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--给类起别名-->
    <typeAliases>
        <typeAlias type="com.fhm.springmybatis.domain.User" alias="_User" />
        <typeAlias type="com.fhm.springmybatis.domain.Order" alias="_Order" />
        <typeAlias type="com.fhm.springmybatis.domain.Item" alias="_Item" />
    </typeAliases>

    <!--引入Mapper-->
    <mappers>
        <mapper resource="userMapper.xml"/>
        <mapper resource="orderMapper.xml"/>
        <mapper resource="itemMapper.xml"/>
    </mappers>
</configuration>